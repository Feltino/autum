@font-face
  font-family: 'Museo Sans Cyrl 100';
  src: url('../fonts/ms-100.eot');
  src: url('../fonts/ms-100.eot?#iefix') format('embedded-opentype'),
          url('../fonts/ms-100.woff') format('woff'),
          url('../fonts/ms-100.ttf') format('truetype'),
          url('../fonts/ms-100.svg#FontName') format('svg');
  font-style: normal;
  font-weight: 100;

